<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <!--<context:annotation-config/>-->

    <dubbo:application name="riskLimitFront"/>
    <dubbo:registry
            id="zkRisk"
            group="dubbo-riskLimitServer-online"
            address="zookeeper://riskzk1:2171?backup=riskzk2:2172,riskzk3:2173"
            default="true"
    />
    <dubbo:consumer timeout="20000"/>

    <dubbo:reference interface="com.hpay.risk.limit.api.LimitCheckServiceApi"
                     id="limitCheckServiceApi" check="false"/>
    <dubbo:reference interface="com.hpay.risk.limit.api.LimitUserManageApi" id="limitUserManageApi"
                     check="false"/>
    <dubbo:reference interface="com.hpay.risk.limit.api.LimitDimensionManageApi"
                     id="limitDimensionManageApi" check="false"/>
</beans>